        "690a111833650497c4687e51": {
          "type": "code",
          "data": {
            "name": "",
            "code": "try {\n  if (last_event?.type?.startsWith('DTMF ')) {\n    digit = last_event.type.slice(-1);\n\n    if (!isNaN(digit)) { // If the pressed key is a digit\n      code += digit;\n      code_length = code.length;\n\n      if (code.length === 11) {\n        caller_id = code;\n        if (caller_id.startsWith('07')) {\n          return 'complete';\n        } else {\n          return 'invalid';\n        }\n      }\n    } else if (digit === '#') { // If the pressed key is '#'\n      caller_id = code; // Use the accumulated digits as the caller_id\n      if (caller_id.startsWith('07')) {\n        return 'complete';\n      } else {\n        return 'invalid'; // If '#' is pressed but the number doesn't start with '07'\n      }\n    }\n  } else {\n    return 'notdtmf';\n  }\n} catch (error) {\n  js_error = error.toString();\n  return \"error\";\n}",

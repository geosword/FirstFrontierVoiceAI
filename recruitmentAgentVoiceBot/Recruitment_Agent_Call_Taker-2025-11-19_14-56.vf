      "code": "export default async function main(args) {\n  /**\n   * Function built by Juan Carlos Quintero - Moonside AI\n   * Website: https://moonside.ai\n   * YouTube: https://www.youtube.com/@juancarlosqr\n   * Voiceflow Certified Expert: https://www.voiceflow.com/contributors/juan-carlos-quintero\n   *\n   * Function to send SMS with Twilio's Messaging API\n   * Docs: https://www.twilio.com/docs/messaging/api/message-resource\n   *\n   * @param {string} message - SMS message content.\n   * @param {string} toNumber - Recipient's phone number (must be in E.164 format).\n   * @param {string} fromNumber - Twilio phone number (must be in E.164 format).\n   * @param {string} authToken - Twilio Auth Token.\n   * @param {string} accountSID - Twilio Account SID.\n   * @returns {string} response - The response body of the API call.\n   */\n\n  // Extract input variables\n  const { accountSID, authToken, fromNumber, toNumber, message } = args.inputVars;\n\n  // Trim input variables\n  const trimmedMessage = message.trim();\n  const trimmedToNumber = toNumber.trim();\n  const trimmedFromNumber = fromNumber.trim();\n  const trimmedAuthToken = authToken.trim();\n  const trimmedAccountSID = accountSID.trim();\n\n  // Validate input variables\n  if (!trimmedMessage || !trimmedToNumber || !trimmedFromNumber || !trimmedAuthToken || !trimmedAccountSID) {\n    return {\n      trace: [\n        {\n          type: 'debug',\n          payload: {\n            message: 'Twilio Function Error: All input variables are required and cannot be blank.'\n          }\n        }\n      ],\n      next: {\n        path: 'error'\n      }\n    };\n  }\n\n  try {\n    // Manually encode credentials in Base64\n    const credentials = base64Encode(`${trimmedAccountSID}:${trimmedAuthToken}`);\n\n    // Manually construct the form-urlencoded body\n    const body =\n      'From=' +\n      urlEncode(trimmedFromNumber) +\n      '&To=' +\n      urlEncode(trimmedToNumber) +\n      '&Body=' +\n      urlEncode(trimmedMessage);\n\n    // Set request URL\n    const requestUrl = `https://api.twilio.com/2010-04-01/Accounts/${trimmedAccountSID}/Messages.json`\n\n    // Send SMS via Twilio API\n    const response = await fetch(requestUrl, {\n      method: 'POST',\n      headers: {\n        'Authorization': `Basic ${credentials}`,\n        'Content-Type': 'application/x-www-form-urlencoded'\n      },\n      body,\n    });\n\n    // Extract JSON response\n    const responseBody = response.json;\n\n    // Check for Twilio API errors\n    if (response.status < 200 || response.status >= 300) {\n      return {\n        trace: [\n          {\n            type: 'debug',\n            payload: {\n              message: `Twilio API call error: ${JSON.stringify(responseBody)}`\n            }\n          }\n        ],\n        next: {\n          path: 'error'\n        }\n      };\n    }\n\n    // Return success response\n    return {\n      outputVars: {\n        response: JSON.stringify(responseBody),\n      },\n      trace: [\n        {\n          type: 'debug',\n          payload: {\n            message: 'Twilio API call successful: SMS sent'\n          }\n        }\n      ],\n      next: {\n        path: 'success'\n      }\n    };\n  } catch (error) {\n    // Handle fetch or unexpected errors\n    return {\n      trace: [\n        {\n          type: 'debug',\n          payload: {\n            message: `Twilio AI call exception: ${error.message}`\n          }\n        }\n      ],\n      next: {\n        path: 'error'\n      }\n    };\n  }\n\n  // Custom function to replace `encodeURIComponent`\n  function urlEncode(str) {\n    return str\n      .split('')\n      .map((char) => {\n        const code = char.charCodeAt(0);\n        if (\n          (code >= 48 && code <= 57) || // 0-9\n          (code >= 65 && code <= 90) || // A-Z\n          (code >= 97 && code <= 122) || // a-z\n          code === 45 || // -\n          code === 46 || // .\n          code === 95 || // _\n          code === 126 // ~\n        ) {\n          return char;\n        }\n        return '%' + code.toString(16).toUpperCase();\n      })\n      .join('');\n  };\n\n  // Custom base64 encoding function\n  function base64Encode(str) {\n    const chars = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/';\n    let result = '';\n    let i = 0;\n    while (i < str.length) {\n      const a = str.charCodeAt(i++) || 0;\n      const b = str.charCodeAt(i++) || 0;\n      const c = str.charCodeAt(i++) || 0;\n\n      const triplet = (a << 16) | (b << 8) | c;\n\n      result +=\n        chars[(triplet >> 18) & 0x3f] +\n        chars[(triplet >> 12) & 0x3f] +\n        chars[(triplet >> 6) & 0x3f] +\n        chars[triplet & 0x3f];\n    }\n\n    const padding = str.length % 3;\n    return padding ? result.slice(0, padding - 3) + '==='.slice(padding) : result;\n  };\n}",
